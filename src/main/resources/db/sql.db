CREATE TABLE `sx_triggers` (
  `ID` bigint(10) NOT NULL AUTO_INCREMENT COMMENT '主键ID',
  `JOB_NAME` varchar(20) DEFAULT NULL COMMENT '任务名称',
  `JOB_GROUP` varchar(20) DEFAULT NULL COMMENT '任务组',
  `JOB_START_DATE` datetime DEFAULT NULL COMMENT '任务开始时间',
  `JOB_END_DATE` datetime DEFAULT NULL COMMENT '任务结束时间',
  `DESCRIPTION` varchar(20) DEFAULT NULL COMMENT '任务描述',
  `CRON_EXPRESSION` varchar(20) DEFAULT NULL COMMENT '时间表达式',
  `REQUEST_URL` varchar(256) DEFAULT NULL COMMENT '请求地址',
  `REQUEST_BODY` varchar(20) DEFAULT NULL COMMENT 'body参数(json格式)',
  `JOB_STATUS` tinyint(1) DEFAULT NULL COMMENT '1：开启，0：关闭',
  `REMARK` varchar(500) DEFAULT NULL COMMENT '备注',
  `CREATE_USER` varchar(20) DEFAULT NULL COMMENT '创建人',
  `CREATE_DATE` datetime DEFAULT NULL,
  `MODIFY_USER` varchar(20) DEFAULT NULL COMMENT '修改人',
  `MODIFY_DATE` datetime DEFAULT NULL,
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='任务调度表';